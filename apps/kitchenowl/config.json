{
  "$schema": "https://schemas.runtipi.io/v2/app-info.json",
  "name": "KitchenOwl",
  "id": "kitchenowl",
  "available": true,
  "short_desc": "Self-hosted grocery list and recipe manager",
  "author": "Tom Bursch",
  "port": 8080,
  "categories": ["utilities"],
  "description": "KitchenOwl keeps grocery lists and recipes organized with shared access, meal planning, and a responsive web UI.",
  "tipi_version": 2,
  "version": "v0.7.6",
  "source": "https://github.com/TomBursch/kitchenowl",
  "website": "https://kitchenowl.org/",
  "exposable": true,
  "supported_architectures": ["arm64", "amd64"],
  "created_at": 1764229240412,
  "updated_at": 1770935280186,
  "dynamic_config": true,
  "requirements": {
    "ports": [8080],
    "volumes": ["${APP_DATA_DIR}/data"]
  },
  "form_fields": [
    {
      "type": "password",
      "label": "JWT Secret Key",
      "required": true,
      "env_variable": "JWT_SECRET_KEY"
    },
    {
      "type": "url",
      "label": "Frontend URL",
      "required": true,
      "env_variable": "FRONT_URL",
      "default": "https://${APP_DOMAIN}"
    }
  ],
  "min_tipi_version": "4.5.0"
}
