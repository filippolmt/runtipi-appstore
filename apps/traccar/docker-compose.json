{
  "services": [
    {
      "name": "traccar",
      "image": "traccar/traccar:6.10-alpine",
      "internalPort": 8082,
      "hostname": "traccar",
      "environment": {
        "NETWORK_INTERFACE": "${NETWORK_INTERFACE:-0.0.0.0}"
      },
      "addPorts": [
        {
          "hostPort": 5027,
          "containerPort": 5027,
          "tcp": true,
          "udp": false
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/traccar",
          "containerPath": "/opt/traccar/data",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ],
      "isMain": true
    }
  ]
}
